<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

            <record id="loan_config_view_tree" model="ir.ui.view">
                <field name="name">loan.config.view.tree</field>
                <field name="model">loan.config</field>
                <field name="arch" type="xml">
                    <tree string="Loan configuration">
                        <field name="upload_fields" />
                    </tree>
                </field>
            </record>

            <record id="loan_config_view_form" model="ir.ui.view">
                <field name="name">loan.config.view.form</field>
                <field name="model">loan.config</field>
                <field name="arch" type="xml">
                    <form string="Loan Configuration">
                        <sheet>
                            <group>
                                <field name="upload_fields" />
                            </group>
                        </sheet>
                    </form>
                </field>
            </record>
    
    </data>   
</odoo>